<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/animal_product_list_item"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tile_grey_selector"
    android:elevation="5dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/denree_type_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:textColor="@color/FoodOriginBlack"
        android:textSize="18sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/denree_type_details"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:textColor="@color/FoodOriginGrey2"
        android:textSize="14sp" />


    <TextView
        android:id="@+id/denree_type_infos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:textColor="@color/FoodOriginGrey2"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/denree_origin_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:textColor="@color/FoodOriginBlack"
        android:textSize="18sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/denree_origine_details"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:textColor="@color/FoodOriginGrey2"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/denree_origine_infos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:textColor="@color/FoodOriginGrey2"
        android:textSize="14sp" />


</LinearLayout>